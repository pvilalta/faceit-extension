{
  "manifest_version": 3,
  "name": "Faceit Team Winrate Teller",
  "version": "1.0",
  "description": "Récupère les pseudos des joueurs des deux équipes sur une page de match Faceit",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "handgun16.png",
      "48": "handgun48.png",
      "128": "handgun128.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "handgun16.png",
    "48": "handgun48.png",
    "128": "handgun128.png"
  }
}
